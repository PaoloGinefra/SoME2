{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:next && npm run lint:mdx -- --check",
    "format": "npm run lint:next -- --fix && npm run lint:mdx -- --write",
    "lint:next": "next lint --max-warnings 0",
    "lint:mdx": "prettier src/**/*.mdx",
    "test": "npm run build && npm run lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.2",
    "@next/mdx": "^12.2.2",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  }
}
